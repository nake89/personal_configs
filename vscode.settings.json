  "telemetry.enableCrashReporter": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "workbench.colorCustomizations": {
    "editor.selectionHighlightBackground": "#FF79C6",
    // "editor.selectionBackground": "#88a9ffd0"
    "editor.selectionBackground": "#FF79C6"
  },
  "workbench.colorTheme": "Dark-Dracula",
  "prettier.bracketSpacing": false,
  "window.zoomLevel": 0,
  "files.watcherExclude": {
    "**/vendor/**": true
  },
  "prettier.semi": false,
  "vetur.format.defaultFormatterOptions": {
    "prettier": {
      // Prettier option here
      "semi": false
    }
  },
  "vim.easymotion": true,
  "editor.lineNumbers": "relative",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "s"
      ],
      "after": [
        "leader",
        "leader",
        "s"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.easymotionMarkerFontSize": "18",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerBackgroundColor": "black",
  "vim.easymotionMarkerHeight": 18,
  "vim.easymotionMarkerWidthPerChar": 18,
  "vim.easymotionMarkerForegroundColorOneChar": "#FF79C6",
  "vim.easymotionMarkerForegroundColorTwoChar": "#FF79C6",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-w>": false,
    "<C-c>": false,
    "<C-l>": false
  },
  "zenMode.hideStatusBar": false,
  "zenMode.hideLineNumbers": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.nativeFullScreen": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src"
  }
}
